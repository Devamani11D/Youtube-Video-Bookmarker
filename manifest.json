{
    "manifest_version":2,
    "name":"Youtube Video Bookmarker!",
    "description":"It is an extension to bookmark time points in youtube video.",
    "version":"1.0",
    "permissions":["activeTab","declarativeContent","storage","tabs","https://www.youtube.com/*"],
    "options_page":"options.html",
    "background":{
        "scripts":["background.js"],
        "persistent":false
    },
    "externally_connectable":{
        "matches":["*://*.youtube.com/*"]
    },
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html",
        "default_title":"Hello World Extension"
    }
}